{
  "name": "politico-api",
  "version": "1.0.0",
  "description": "Voting App",
  "main": "src/server.ts",
  "author": "Bange Sabastine",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev --files src/server.ts",
    "dev": "tsc -w & nodemon .",
    "test": "jest --coverage",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:refresh": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.3",
    "sequelize-cli": "^6.5.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.7",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "bcrypt": "^5.1.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.2.2",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
